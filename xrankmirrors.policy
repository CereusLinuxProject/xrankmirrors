<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE policyconfig PUBLIC
 "-//freedesktop//DTD PolicyKit Policy Configuration 1.0//EN"
 "http://www.freedesktop.org/standards/PolicyKit/1/policyconfig.dtd">
<policyconfig>

  <action id="xrankmirrors">
    <message>Authentication is required to run the mirror selector</message>
    <message xml:lang="es">Se requiere autenticaci√≥n para ejecutar el selector de espejos</message>

    <icon_name>configurator</icon_name>
    <defaults>
      <allow_any>auth_admin</allow_any>
      <allow_inactive>auth_admin</allow_inactive>
      <allow_active>auth_admin</allow_active>
    </defaults>
    <annotate key="org.freedesktop.policykit.exec.path">/usr/bin/xrankmirrors</annotate>
      </action>

</policyconfig>
